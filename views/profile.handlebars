<div class="page-header">
<h3 class="center-text">Sign Up to Upload Pictures of your Pet!</h3>
</div>
<div class="contactUs">

<form class="signup" role="form"
  action="/process?form=signup" method="POST">

  <input type="hidden" name="_csrf" value="{{csrf}}">
  <div class="row">
    <div class="col-md-2"></div>
    <div class="form-group col-md-8" >
      <label for="fieldEmail" class="control-label">
      Email</label>
      <div>
        <input type="text" class="form-control" id="fieldEmail"
        name="email">
      </div>
    </div> <!-- End of form-group -->
    <div class="col-md-2"></div>
  </div>

  <div class="row">
      <div class="col-md-2"></div>
    <div class="form-group col-md-8">
      <label for="fieldQues" class="control-label">
      Password</label>
      <div>
        <input type="text" class="form-control" id="fieldQues"
        name="ques">
      </div>
    </div> <!-- End of form-group -->
    <div class="col-md-2"></div>
  </div>

  <div class="row">
  <div class="col-md-2"></div>
    <div class="form-group col-md-8">
      <div>
        <button type="submit" class="btn btn-default">Sign Up</button>
      </div>
    </div> <!-- End of form-group -->
    <div class="col-md-2"></div>
  </div>
</form>

</div> 
